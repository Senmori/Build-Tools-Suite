<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="565.0" prefWidth="550.0" tabClosingPolicy="UNAVAILABLE" type="TabPane" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.senmori.btsuite.Controller">
  <tabs>
    <Tab fx:id="buildTab" text="Build Tools">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="532.0" prefWidth="600.0">
               <children>
                  <VBox fx:id="flagVBox" layoutX="14.0" layoutY="25.0" prefHeight="116.0" prefWidth="180.0">
                     <children>
                        <CheckBox fx:id="certCheck" mnemonicParsing="false" text="Disable Certificate Check">
                           <tooltip>
                              <Tooltip text="Disable certificate checks when polling websites." />
                           </tooltip>
                        </CheckBox>
                        <CheckBox fx:id="dontUpdate" mnemonicParsing="false" text="Don't Update">
                           <tooltip>
                              <Tooltip text="Don't update local repository from remote repository." />
                           </tooltip>
                        </CheckBox>
                        <CheckBox fx:id="skipCompile" mnemonicParsing="false" text="Skip Compile">
                           <tooltip>
                              <Tooltip text="Do not compile the final Jar. This will still generate the source and/or javadoc jars." />
                           </tooltip>
                        </CheckBox>
                        <CheckBox fx:id="genSrc" mnemonicParsing="false" text="Generate Sources">
                           <tooltip>
                              <Tooltip text="Generate a source Jar during compilation" />
                           </tooltip>
                        </CheckBox>
                        <CheckBox fx:id="genDoc" mnemonicParsing="false" text="Generate Documentation">
                           <tooltip>
                              <Tooltip text="Generate a javadoc Jar during compilation" />
                           </tooltip>
                        </CheckBox>
                     </children>
                  </VBox>
                  <ChoiceBox fx:id="revChoiceBox" layoutX="436.0" layoutY="25.0" prefWidth="150.0" />
                  <VBox fx:id="outVBox" layoutX="14.0" layoutY="141.0">
                     <children>
                        <Button fx:id="outBrowseBtn" mnemonicParsing="false" text="Browse..." />
                     </children>
                  </VBox>
               </children></AnchorPane>
      </content>
    </Tab>
    <Tab fx:id="consoleTab" text="Console">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
      </content>
    </Tab>
  </tabs>
</fx:root>
